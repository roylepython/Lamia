            </button>
            <button class="action-btn" onclick="openUserMenu()">
                <i class="fas fa-user-circle"></i>
            </button>
        </div>
    </header>

    <!-- Enterprise Sidebar -->
    <nav class="enterprise-sidebar">
        <div class="nav-section">
            <div class="nav-title">Dashboard</div>
            <a href="#dashboard" class="nav-item active">
                <i class="fas fa-tachometer-alt"></i>
                Overview
            </a>
            <a href="#monitoring" class="nav-item">
                <i class="fas fa-chart-line"></i>
                Real-time Monitoring
            </a>
            <a href="#analytics" class="nav-item">
                <i class="fas fa-analytics"></i>
                Analytics Engine
            </a>
        </div>

        <div class="nav-section">
            <div class="nav-title">Server Management</div>
            <a href="#services" class="nav-item">
                <i class="fas fa-server"></i>
                Services Control
            </a>
            <a href="#processes" class="nav-item">
                <i class="fas fa-microchip"></i>
                Process Manager
            </a>
            <a href="#resources" class="nav-item">
                <i class="fas fa-memory"></i>
                Resource Monitor
            </a>
            <a href="#logs" class="nav-item">
                <i class="fas fa-file-alt"></i>
                System Logs
            </a>
        </div>

        <div class="nav-section">
            <div class="nav-title">Security</div>
            <a href="#icewall" class="nav-item">
                <i class="fas fa-shield-alt"></i>
                ICEWALL Fortress
