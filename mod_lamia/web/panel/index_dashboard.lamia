@application MedusaServUltimateDashboard {
    @version "0.3.0c-REVOLUTION"
    @type "ultimate_enterprise_dashboard"
    @architecture "revolutionary_modular_ecosystem"
    @user_focus "customer_first_experience"
    @integrations ["apexcharts", "3d_emotions", "gif3d_revolutionary", "icewall_fortress", "triforce_database", "medusacp_cpp_engine"]
    @quality_standard "yorkshire_champion_professional"
}

<!-- Include C++ Engine Integration -->
@include "medusacp-integration.lamia"

@page UltimateEnterpriseDashboard {
    @layout "revolutionary_adaptive_grid"
    @theme "medusa_ultimate_professional"
    @real_time_polling "500ms"
    @charts_library "apexcharts_3d_enhanced"
    @animations "gif3d_revolutionary_enhanced"
    @security "icewall_fortress_integrated"
    @intelligence "ai_powered_insights"
    @responsive "mobile_first_yorkshire_champion"
}

@head {
    <title>ðŸ”® MedusaServ Enterprise Dashboard - Yorkshire Champion Standards</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- Professional Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    
    <!-- Icons -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    
    <!-- ApexCharts Library -->
    <script src="https://cdn.jsdelivr.net/npm/apexcharts@latest"></script>
    
    <!-- AOS Animations -->
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    
    @style {
        :root {
            /* Core Brand Colors */
            --primary-purple: #8b3f8b;
            --gif3d-pink: #ff69b4;
            --space-dark: #0a0a0a;
            --medusa-blue: #2196f3;
            --success-green: #4caf50;
            --warning-orange: #ff9800;
            --danger-red: #f44336;
            --yorkshire-gold: #ffd700;
            --champion-silver: #c0c0c0;
            --enterprise-navy: #1a237e;
            
            /* Glass Morphism */
            --glass-bg: rgba(255, 255, 255, 0.08);
            --glass-bg-strong: rgba(255, 255, 255, 0.12);
            --glass-border: rgba(255, 255, 255, 0.15);
            --glass-border-strong: rgba(255, 255, 255, 0.25);
            
            /* Typography */
            --text-primary: #ffffff;
            --text-secondary: rgba(255, 255, 255, 0.8);
            --text-muted: rgba(255, 255, 255, 0.6);
            --text-accent: var(--gif3d-pink);
            
            /* Shadows & Effects */
            --shadow-depth: 0 12px 48px rgba(139, 63, 139, 0.4);
            --shadow-elevation: 0 8px 32px rgba(0, 0, 0, 0.3);
            --glow-purple: 0 0 30px rgba(139, 63, 139, 0.6);
            --glow-pink: 0 0 30px rgba(255, 105, 180, 0.6);
            
            /* Component Sizes */
            --speedometer-size: 350px;
            --card-radius: 20px;
            --button-radius: 12px;
            --border-width: 2px;
            
            /* Animations */
            --transition-fast: 0.2s ease;
            --transition-medium: 0.4s cubic-bezier(0.4, 0, 0.2, 1);
            --transition-slow: 0.8s cubic-bezier(0.25, 0.46, 0.45, 0.94);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            outline: none !important;
        }

        body {
            font-family: 'Montserrat', sans-serif;
            background: var(--space-dark);
            color: var(--text-primary);
            overflow-x: hidden;
            min-height: 100vh;
        }

        /* Include Modular Components */
        @import url('./inc/header.lamia');
        @import url('./inc/sidemenu.lamia');

        /* Universe Background with 3D Emotions */
        .universe-background {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: -10;
            overflow: hidden;
        }
        
        .spinning-universe {
            position: absolute;
            width: 200%;
            height: 200%;
            top: -50%;
            left: -50%;
            animation: universeRotate 120s linear infinite;
        }
        
        .universe-layer {
            position: absolute;
            width: 100%;
            height: 100%;
            background: radial-gradient(circle at center, 
                transparent 0%, 
                rgba(139, 63, 139, 0.05) 25%, 
                rgba(255, 105, 180, 0.03) 50%, 
                transparent 75%);
            animation: layerFloat 15s ease-in-out infinite;
        }
        
        .universe-layer:nth-child(2) {
            animation-delay: -5s;
            animation-duration: 20s;
        }
        
        .universe-layer:nth-child(3) {
            animation-delay: -10s;
            animation-duration: 25s;
        }
        
        .universe-layer:nth-child(4) {
            animation-delay: -15s;
            animation-duration: 30s;
        }
        
        @keyframes universeRotate {
            from { transform: rotate(0deg); }
            to { transform: rotate(360deg); }
        }
        
        @keyframes layerFloat {
            0%, 100% { opacity: 0.5; transform: scale(1); }
            50% { opacity: 1; transform: scale(1.1); }
        }
        
        /* Main Dashboard Content */
        .dashboard-content {
            margin-left: 280px;
            margin-top: 100px;
            padding: 2rem;
            min-height: calc(100vh - 100px);
            position: relative;
            transition: margin-top 0.3s ease;
        }
        
        /* Adjust for update banner */
        .dashboard-content.with-update-banner {
            margin-top: 170px;
        }

        /* Advanced Dashboard Header */
        .dashboard-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin-bottom: 2rem;
            padding: 2rem 2.5rem;
            background: var(--glass-bg-strong);
            border: 2px solid var(--glass-border-strong);
            border-radius: var(--card-radius);
            backdrop-filter: blur(25px);
            box-shadow: var(--shadow-elevation);
            position: relative;
            overflow: hidden;
        }
        
        .dashboard-header::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(139, 63, 139, 0.1), transparent);
            animation: headerGlow 3s ease-in-out infinite;
        }
        
        @keyframes headerGlow {
            0% { left: -100%; }
            100% { left: 100%; }
        }

        .dashboard-title {
            display: flex;
            align-items: center;
            gap: 1rem;
        }

        .dashboard-title h1 {
            font-size: 2.5rem;
            font-weight: 800;
            background: linear-gradient(45deg, var(--primary-purple), var(--gif3d-pink), var(--yorkshire-gold));
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-size: 200% 200%;
            animation: titleShimmer 3s ease-in-out infinite;
            text-shadow: 0 0 30px rgba(139, 63, 139, 0.5);
        }
        
        @keyframes titleShimmer {
            0%, 100% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
        }

        .dashboard-stats {
            display: flex;
            gap: 2rem;
        }

        .revolutionary-stat {
            display: flex;
            align-items: center;
            gap: 1rem;
            padding: 1.5rem;
            background: var(--glass-bg-strong);
            border: 2px solid var(--glass-border);
            border-radius: var(--card-radius);
            backdrop-filter: blur(20px);
            transition: all var(--transition-medium);
            position: relative;
            overflow: hidden;
        }
        
        .revolutionary-stat:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-depth);
            border-color: var(--primary-purple);
        }
        
        .revolutionary-stat::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, 
                transparent, 
                rgba(139, 63, 139, 0.1), 
                transparent);
            transition: left 0.5s ease;
        }
        
        .revolutionary-stat:hover::before {
            left: 100%;
        }
        
        .stat-icon {
            width: 50px;
            height: 50px;
            background: linear-gradient(45deg, var(--primary-purple), var(--gif3d-pink));
            border-radius: 15px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
            color: white;
            box-shadow: var(--glow-purple);
        }
        
        .stat-info {
            flex: 1;
            text-align: left;
        }

        .stat-value {
            font-size: 1.8rem;
            font-weight: 800;
            color: var(--text-primary);
            margin-bottom: 0.25rem;
            text-shadow: 0 0 10px rgba(139, 63, 139, 0.5);
        }

        .stat-label {
            font-size: 0.9rem;
            color: var(--text-secondary);
            font-weight: 500;
            margin-bottom: 0.25rem;
        }
        
        .stat-trend {
            font-size: 0.75rem;
            font-weight: 600;
            padding: 0.25rem 0.5rem;
            border-radius: 10px;
            display: inline-block;
        }
        
        .stat-trend.up {
            background: rgba(76, 175, 80, 0.2);
            color: var(--success-green);
        }
        
        .stat-trend.stable {
            background: rgba(33, 150, 243, 0.2);
            color: var(--medusa-blue);
        }
        
        .stat-trend.secure {
            background: rgba(139, 63, 139, 0.2);
            color: var(--primary-purple);
        }
        
        .stat-trend.excellent {
            background: rgba(255, 215, 0, 0.2);
            color: var(--yorkshire-gold);
        }

        /* Main Dashboard Grid */
        .dashboard-grid {
            display: grid;
            grid-template-columns: 1fr var(--speedometer-size) 1fr;
            grid-template-rows: auto auto auto;
            gap: 2rem;
            margin-bottom: 2rem;
        }

        /* Left Side - Connection Charts */
        .connection-charts {
            display: flex;
            flex-direction: column;
            gap: 1.5rem;
        }

        /* Revolutionary Speedometer */
        .speedometer-container {
            grid-row: 1 / 3;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            background: var(--glass-bg-strong);
            border: 3px solid var(--glass-border-strong);
            border-radius: 25px;
            padding: 2.5rem;
            backdrop-filter: blur(30px);
            position: relative;
            overflow: hidden;
            box-shadow: var(--shadow-depth);
        }
        
        .speedometer-container::after {
            content: '';
            position: absolute;
            top: 10px;
            left: 10px;
            right: 10px;
            bottom: 10px;
            border: 1px solid rgba(255, 105, 180, 0.3);
            border-radius: 20px;
            pointer-events: none;
        }

        .speedometer-container::before {
            content: '';
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: conic-gradient(from 0deg, transparent, rgba(139, 63, 139, 0.1), transparent);
            animation: speedometerGlow 4s linear infinite;
            z-index: -1;
        }

        @keyframes speedometerGlow {
            from { transform: rotate(0deg); }
            to { transform: rotate(360deg); }
        }

        .speedometer-title {
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--primary-purple);
            margin-bottom: 1rem;
            text-align: center;
        }

        .speedometer {
            width: var(--speedometer-size);
            height: calc(var(--speedometer-size) * 0.65);
            position: relative;
            border-radius: var(--speedometer-size) var(--speedometer-size) 0 0;
            background: conic-gradient(from 0deg at center bottom,
                var(--success-green) 0deg 60deg,
                var(--yorkshire-gold) 60deg 120deg,
                var(--warning-orange) 120deg 150deg,
                var(--danger-red) 150deg 180deg
            );
            padding: 8px;
            overflow: hidden;
        }
        
        .speedometer-track {
            width: 100%;
            height: 100%;
            border-radius: inherit;
            background: var(--space-dark);
            position: relative;
        }
        
        .speedometer-markers {
            position: absolute;
            width: 100%;
            height: 100%;
        }
        
        .speedometer-marker {
            position: absolute;
            bottom: 0;
            left: 50%;
            transform-origin: bottom center;
            width: 2px;
            height: 30px;
            background: rgba(255, 255, 255, 0.8);
            border-radius: 1px;
        }
        
        .speedometer-marker.major {
            width: 3px;
            height: 40px;
            background: rgba(255, 255, 255, 1);
        }

        .speedometer-needle {
            position: absolute;
            bottom: 8px;
            left: 50%;
            width: 5px;
            height: calc(var(--speedometer-size) * 0.45);
            background: linear-gradient(180deg, 
                var(--gif3d-pink) 0%, 
                var(--primary-purple) 50%, 
                var(--danger-red) 100%);
            transform-origin: bottom center;
            transform: translateX(-50%) rotate(-90deg);
            transition: transform var(--transition-slow);
            border-radius: 3px 3px 0 0;
            box-shadow: 
                0 0 20px rgba(255, 105, 180, 0.8),
                0 0 40px rgba(139, 63, 139, 0.6),
                inset 0 0 10px rgba(255, 255, 255, 0.3);
            animation: needleGlow 1.5s ease-in-out infinite alternate;
            z-index: 10;
        }
        
        .speedometer-needle::before {
            content: '';
            position: absolute;
            top: -3px;
            left: -1px;
            right: -1px;
            height: 8px;
            background: var(--yorkshire-gold);
            border-radius: 50%;
            box-shadow: 0 0 15px var(--yorkshire-gold);
        }

        @keyframes needleGlow {
            from { box-shadow: 0 0 20px rgba(244, 67, 54, 0.5); }
            to { box-shadow: 0 0 40px rgba(244, 67, 54, 0.8); }
        }

        .speedometer-center {
            position: absolute;
            bottom: -2px;
            left: 50%;
            width: 28px;
            height: 28px;
            background: radial-gradient(circle, 
                var(--yorkshire-gold) 0%, 
                var(--primary-purple) 40%, 
                var(--gif3d-pink) 100%);
            border-radius: 50%;
            transform: translateX(-50%);
            box-shadow: 
                0 0 20px rgba(255, 215, 0, 0.8),
                0 0 40px rgba(139, 63, 139, 0.6),
                inset 0 0 10px rgba(255, 255, 255, 0.4);
            border: 3px solid rgba(255, 255, 255, 0.8);
            z-index: 15;
            animation: centerPulse 2s ease-in-out infinite;
        }
        
        @keyframes centerPulse {
            0%, 100% { transform: translateX(-50%) scale(1); }
            50% { transform: translateX(-50%) scale(1.1); }
        }

        .speedometer-value {
            position: absolute;
            bottom: -40px;
            left: 50%;
            transform: translateX(-50%);
            font-size: 2rem;
            font-weight: 700;
            color: var(--text-primary);
            text-align: center;
        }

        .speedometer-label {
            position: absolute;
            bottom: -70px;
            left: 50%;
            transform: translateX(-50%);
            font-size: 0.9rem;
            color: var(--text-secondary);
            text-align: center;
        }

        /* Right Side - 3D Pie Chart */
        .pie-chart-container {
            grid-row: 1 / 3;
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 20px;
            padding: 1.5rem;
            backdrop-filter: blur(20px);
        }

        .chart-container {
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 15px;
            padding: 1.5rem;
            backdrop-filter: blur(20px);
            margin-bottom: 1rem;
        }

        .chart-title {
            font-size: 1.1rem;
            font-weight: 600;
            color: var(--primary-purple);
            margin-bottom: 1rem;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        /* Service Categories Grid */
        .services-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 2rem;
            margin-bottom: 2rem;
        }

        .service-category {
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 20px;
            padding: 2rem;
            backdrop-filter: blur(20px);
            transition: all 0.3s ease;
        }

        .service-category:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-depth);
        }

        .category-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin-bottom: 1.5rem;
        }

        .category-title {
            font-size: 1.3rem;
            font-weight: 600;
            color: var(--primary-purple);
            display: flex;
            align-items: center;
            gap: 0.75rem;
        }

        .category-count {
            background: linear-gradient(45deg, var(--primary-purple), var(--gif3d-pink));
            color: white;
            padding: 0.25rem 0.75rem;
            border-radius: 15px;
            font-size: 0.8rem;
            font-weight: 500;
        }

        .service-list {
            display: grid;
            gap: 0.75rem;
        }

        .service-item {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 0.75rem;
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid var(--glass-border);
            border-radius: 10px;
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .service-item:hover {
            background: rgba(139, 63, 139, 0.1);
            border-color: var(--primary-purple);
            transform: translateX(5px);
        }

        .service-info {
            display: flex;
            align-items: center;
            gap: 0.75rem;
        }

        .service-icon {
            width: 32px;
            height: 32px;
            background: linear-gradient(45deg, var(--primary-purple), var(--gif3d-pink));
            border-radius: 8px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 0.9rem;
        }

        .service-details {
            display: flex;
            flex-direction: column;
        }

        .service-name {
            font-weight: 500;
            color: var(--text-primary);
            font-size: 0.9rem;
        }

        .service-description {
            font-size: 0.7rem;
            color: var(--text-secondary);
        }

        .service-status {
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .status-indicator {
            width: 8px;
            height: 8px;
            border-radius: 50%;
            animation: statusPulse 2s ease-in-out infinite;
        }

        .status-indicator.online {
            background: var(--success-green);
        }

        .status-indicator.warning {
            background: var(--warning-orange);
        }

        .status-indicator.offline {
            background: var(--danger-red);
        }

        .status-text {
            font-size: 0.8rem;
            font-weight: 500;
        }

        /* Plugin Manager Section */
        .plugin-manager {
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 20px;
            padding: 2rem;
            backdrop-filter: blur(20px);
            margin-bottom: 2rem;
        }

        .plugin-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin-bottom: 2rem;
        }

        .plugin-title {
            font-size: 1.5rem;
            font-weight: 600;
            color: var(--primary-purple);
            display: flex;
            align-items: center;
            gap: 1rem;
        }

        .plugin-actions {
            display: flex;
            gap: 1rem;
        }

        .plugin-btn {
            padding: 0.75rem 1.5rem;
            background: linear-gradient(45deg, var(--primary-purple), var(--gif3d-pink));
            border: none;
            border-radius: 10px;
            color: white;
            font-weight: 500;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .plugin-btn:hover {
            transform: translateY(-2px);
            box-shadow: var(--shadow-depth);
        }

        .plugin-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 1.5rem;
        }

        .plugin-card {
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid var(--glass-border);
            border-radius: 15px;
            padding: 1.5rem;
            transition: all 0.3s ease;
        }

        .plugin-card:hover {
            background: rgba(139, 63, 139, 0.05);
            border-color: var(--primary-purple);
            transform: translateY(-3px);
        }

        .plugin-card-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin-bottom: 1rem;
        }

        .plugin-name {
            font-weight: 600;
            color: var(--text-primary);
        }

        .plugin-version {
            font-size: 0.8rem;
            color: var(--text-secondary);
            background: var(--glass-bg);
            padding: 0.25rem 0.5rem;
            border-radius: 8px;
        }

        .plugin-description {
            color: var(--text-secondary);
            font-size: 0.9rem;
            line-height: 1.4;
            margin-bottom: 1rem;
        }

        .plugin-controls {
            display: flex;
            gap: 0.5rem;
        }

        .plugin-control-btn {
            flex: 1;
            padding: 0.5rem;
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 8px;
            color: var(--text-primary);
            cursor: pointer;
            transition: all 0.3s ease;
            font-size: 0.8rem;
        }

        .plugin-control-btn:hover {
            background: var(--primary-purple);
        }

        .plugin-control-btn.active {
            background: var(--success-green);
        }

        .plugin-control-btn.danger {
            background: var(--danger-red);
        }

        /* Quick Actions Panel */
        .quick-actions-panel {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 1.5rem;
            margin-bottom: 2rem;
        }

        .quick-action {
            background: var(--glass-bg);
            border: 1px solid var(--glass-border);
            border-radius: 15px;
            padding: 1.5rem;
            backdrop-filter: blur(20px);
            text-align: center;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .quick-action:hover {
            background: var(--primary-purple);
            transform: translateY(-5px);
            box-shadow: var(--shadow-depth);
        }

        .quick-action-icon {
            font-size: 2rem;
            margin-bottom: 1rem;
            color: var(--primary-purple);
            transition: color 0.3s ease;
        }

        .quick-action:hover .quick-action-icon {
            color: white;
        }

        .quick-action-title {
            font-weight: 600;
            margin-bottom: 0.5rem;
        }

        .quick-action-description {
            font-size: 0.8rem;
            color: var(--text-secondary);
        }

        /* Responsive Design */
        @media (max-width: 1200px) {
            .dashboard-grid {
                grid-template-columns: 1fr;
                grid-template-rows: auto;
            }
            
            .speedometer-container {
                grid-row: auto;
            }
        }

        @media (max-width: 1024px) {
            .dashboard-content {
                margin-left: 0;
                margin-top: 100px;
            }
        }
        
        @media (max-width: 768px) {
