        // Revolutionary Speedometer Update
        function updateSpeedometer(value) {
            const needle = document.getElementById('performanceNeedle');
            const valueDisplay = document.getElementById('performanceValue');
            
            if (needle && valueDisplay) {
                // Smooth value transition
                const smoothValue = Math.max(0, Math.min(100, value));
                
                // Convert value (0-100) to rotation (-90 to 90 degrees)
                const rotation = (smoothValue * 1.8) - 90;
                needle.style.transform = `translateX(-50%) rotate(${rotation}deg)`;
                
                // Update display with smooth animation
                valueDisplay.textContent = `${smoothValue.toFixed(1)}%`;
                
                // Dynamic color based on performance
                let performanceColor = '';
                if (smoothValue >= 90) {
                    performanceColor = 'var(--success-green)';
                } else if (smoothValue >= 70) {
                    performanceColor = 'var(--yorkshire-gold)';
                } else if (smoothValue >= 50) {
                    performanceColor = 'var(--warning-orange)';
                } else {
                    performanceColor = 'var(--danger-red)';
                }
                
                valueDisplay.style.color = performanceColor;
                valueDisplay.style.textShadow = `0 0 15px ${performanceColor}`;
                
                // Update needle glow
                const glowIntensity = smoothValue / 100;
                needle.style.boxShadow = `
                    0 0 ${20 * glowIntensity}px rgba(255, 105, 180, ${0.8 * glowIntensity}),
                    0 0 ${40 * glowIntensity}px rgba(139, 63, 139, ${0.6 * glowIntensity}),
                    inset 0 0 10px rgba(255, 255, 255, ${0.3 * glowIntensity})
                `;
            }
        }

        // Real-time updates (every 1 second)
        function startRealTimeUpdates() {
            setInterval(() => {
                // Update performance data
                performanceData = Math.random() * 100;
                updateSpeedometer(performanceData);

                // Update connection data
                const now = new Date().getTime();
                connectionsData.push([now, Math.floor(Math.random() * 500) + 100]);
                if (connectionsData.length > 50) connectionsData.shift();

                // Update dropped sessions data
                droppedSessionsData.push([now, Math.floor(Math.random() * 20)]);
                if (droppedSessionsData.length > 50) droppedSessionsData.shift();

                // Update resource data
                resourceData = [
                    Math.random() * 50 + 20,
                    Math.random() * 40 + 15,
                    Math.random() * 30 + 10
                ];

                // Update charts would go here
                updateDashboardStats();
            }, 1000);
        }

